{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "restrictResetPasswordTokenScope": true,
  "emailVerificationRequired": true,
  "properties": {
    "isPasswordChanged": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "roles": {
      "type": "hasMany",
      "model": "Role",
      "foreignKey": "principalId",
      "through": "RoleMapping"
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
